<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox stylesheets="styles.css"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="cz.vse.sudoku.ui.GameController"
      prefHeight="400.0" prefWidth="600.0">
    <Label alignment="TOP_LEFT" onMouseClicked="#onBack">Back to Menu</Label>

    <GridPane fx:id="sudokuGrid">

    </GridPane>


    <HBox alignment="BOTTOM_CENTER"
          spacing="15">
        <Label onMouseClicked="#onRegenerate">Regenerate</Label>
        <Label onMouseClicked="#onSave">Save</Label>
        <Label onMouseClicked="#onHelp">Help</Label>
    </HBox>

</VBox>
